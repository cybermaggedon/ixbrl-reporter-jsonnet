"\n.page { page-break-after: always; }\nbody { font-family: Helvetica, Georgia, sans-serif; font-size: medium; }\n@media screen, projection, tv {\nbody { margin: 2% 4% 2% 4%; background-color: gray; }\ndiv.page {\nbackground-color: white; padding: 2em;\n/* CSS hack for cross browser minimum height */ min-height: 29.7cm;\nheight: 29.7cm; width: 21cm;\nmargin: 2em 0; }\n}\n.titlepage .company-number { text-align: right; font-size: small; font-weight: bold; }\n.titlepage img { margin: auto; width: 25%; display: block; padding-top: 12em; padding-left: 0em; padding-bottom: 1em; padding-right: 0em; }\n.titlepage .company-name { text-align: center; font-size: xx-large; font-weight: bold; padding-top: 1em; padding-left: 0em; padding-bottom: 1em; padding-right: 0em; }\n.titlepage .title { text-align: center; font-size: large; font-weight: bold; padding: 0.5rem; }\n.titlepage .subtitle { text-align: center; font-size: large; font-weight: bold; padding: 0.5rem; }\n.page .header { text-align: center; font-weight: bold; }\n.page .header div:last-child { padding-bottom: 0.5rem; }\n.page .header hr { margin-bottom: 2rem; }\n.sheet { width: 80%; border-collapse: separate; }\n.sheet td { padding-top: 0em; padding-left: 1.2em; padding-bottom: 0em; padding-right: 1.2em; }\n.spacetable { table-layout: fixed; border-collapse: separate; }\n.spacetable td { padding-top: 0em; padding-left: 1.2em; padding-bottom: 0em; padding-right: 1.2em; }\ntable { border-spacing: 1rem 1rem; border-collapse: separate; }\n.table { border-spacing: 0.2rem 0rem; border-collapse: separate; font-size: medium; margin-left: auto; margin-right: auto; }\n.company-info { border-collapse: separate; border-spacing: 0em; display: table; table-layout: fixed; }\n.company-info tr td { vertical-align: top; padding: 0.5em; }\n.company-info tr td.tag { width: 15em; font-weight: bold; }\n.row { display: table-row; }\n.cell { display: table-cell; }\n.label { width: 50%; padding-right: 1em; font-size: smaller; }\n.label.heading { font-weight: bold; margin-top: 1rem; }\n.note { font-size: smaller; text-align: center; padding-left: 0.2em; padding-right: 0.2em; }\n.note.header { font-size: smaller; font-weight: bold; text-align: center; padding-left: 1em; padding-right: 1em; }\n.label.breakdown.heading { margin-bottom: 0.2rem; }\n.value { font-family: Source Code Pro, monospace, Courier New; width: 20%; padding-left: 1em; padding-right: 1em; margin-right: 1em; font-size: small; }\n@media print { .label { font-size: small; width: 40%; } .value { font-family: Source Code Pro, monospace; width: 20%; font-size: small; } .company-info { font-size: medium; } table.sheet { margin-left: 20%; } table.spacetable { margin-left: 20%; } body { font-size: 11pt; } .data { width: 8em; } }\n.total.value { margin-top: 1em; }\n.breakdown.total { margin-top: 0.2em; padding-top: 4px; border-top: 1px solid white; }\n.breakdown.total.value { border-top: 1px solid #808080; }\n.breakdown.total.super-total.value { border-top: 5px double #808080; }\n.column.header { padding: 0.5em 1em 0.5em 1em; border-bottom: 0.2em solid black; font-weight: bold; text-align: center; width: 20%; margin-right: 1rem; }\n.currency { text-align: right; padding-left: 1em; padding-right: 1em; width: 20%; margin-right: 1rem; margin-top: 0.25em; }\n.data.value { text-align: right; }\n.data.value.negative { color: #400000; }\n.data.value.nil { color: #a0a0a0; }\n.hidden { display: none; }\n.fact { display: flex; flex-direction: row; margin: 2px; }\n.fact div { margin: 0rem 0rem 0rem 1rem; padding: 4px; }\n.fact .ref { width: 5em; text-align: center; color: white; background-color: #2ca469; border: 2px solid #104020; font-weight: bold; font-size: small; padding-left: 0.1rem; padding-right: 0.1rem; }\n.fact .description { width: 22em; font-size: small; }\n.fact .factvalue { border: 2px solid black; background-color: white; font-family: Source Code Pro, monospace; font-size: small; width: 12rem; padding-left: 1rem; padding-right: 1rem; margin-right: 1rem; }\n.fact .factvalue.false { color: #a0a0a0; }\n.rank-1 { position: relative; left: -6rem; }\n.rank1 { position: relative; left: 4rem; }\n"
